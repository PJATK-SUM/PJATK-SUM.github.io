{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-multi-spaces": 1,
    "no-warning-comments": 1,
    "no-unused-vars": 1,
    "array-bracket-spacing": 1,
    "block-spacing": 1,
    "brace-style": 1,
    "camelcase": 1,
    "comma-spacing": 1,
    "indent": 1,
    "keyword-spacing": 1,
    "key-spacing": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-alert": 1,
    "no-extra-parens": 1,
    "curly": [1, "multi-line"],
    "no-irregular-whitespace": 2,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-case-declarations": 2,
    "no-empty-pattern": 2,
    "no-fallthrough": 2,
    "no-octal": 2,
    "no-new": 2,
    "no-redeclare": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-unused-labels": 2,
    "wrap-iife": 2,
    "strict": 2,
    "no-undef": 2
  }
}
